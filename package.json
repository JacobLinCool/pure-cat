        "pure-cat": "workspace:*"
    },
    "devDependencies": {
        "@types/jest": "^28.1.6",
        "@types/node": "^18.7.4",
        "@types/node-fetch": "^2.6.2",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^28.1.3",
        "prettier": "^2.7.1",
